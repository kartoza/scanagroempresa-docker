            <list>
              <mapping local-root="$PROJECT_DIR$/django_project" remote-root="/home/web/django_project" />
              <mapping local-root="$PROJECT_DIR$/submodules/scanagroempresa/django_app" remote-root="/home/web/extra_app" />
            </list>
          </settings>
        </remote-mappings>
        <remote-mappings server-id="python@ssh://root@localhost:{{ docker_port_forward.scanagroempresa.ssh }}/usr/local/bin/python">
          <settings>
            <list>
              <mapping local-root="$PROJECT_DIR$/django_project" remote-root="/home/web/django_project" />
              <mapping local-root="$PROJECT_DIR$/submodules/scanagroempresa/django_app" remote-root="/home/web/extra_app" />
              <mapping local-root="$PROJECT_DIR$/submodules/scanagroempresa/kobo_sync_service" remote-root="/home/web/kobo_sync_service" />
              <mapping local-root="$PROJECT_DIR$/submodules/scanagroempresa_custom_theme" remote-root="{{ scanagroempresa.environment.SCANAGROEMPRESA_THEME_APP_PATH }}" />
              <mapping local-root="$PROJECT_DIR$/submodules/geonode_custom_theme" remote-root="{{ geonode.environment.GEONODE_THEME_APP_PATH }}" />
            </list>
          </settings>
        </remote-mappings>
      </list>
    </list>
  </component>
</project>
